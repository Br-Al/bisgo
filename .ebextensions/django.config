container_commands:
  01_migrate:
    command: "source /var/app/venv/staging-LQM1lest/bin/activate && python3 manage.py migrate --noinput"
    leader_only: true
  createsuperuser:
    command: "echo \"from django.contrib.auth.models import User; User.objects.create_superuser('admin', 'admin@kamerbusinesscenter.com', 'pass')\" | python3 manage.py shell"
option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: kamerbusinesscenter.wsgi:application
  aws:elasticbeanstalk:environment:proxy:staticfiles:
    /static: static
packages: 
  yum:
    python3-devel: []
    mariadb-devel: []